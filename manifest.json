{
  "name": "Yoopla X",
  "version": "1.0",
  "description": "Import your Linkedin candidates directly to Yoopla ATS.",
  "default_locale": "en",
  "manifest_version": 3,
  "version_name": "0.0.0.1",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "/images/logo-16x16.png",
      "128": "/images/logo-32x32.png",
      "32": "/images/logo-128x128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ],
  "host_permissions": [
    "http://localhost:3000/*"
  ],
  "permissions": [
    "activeTab",
    "scripting",
    "cookies",
    "tabs"
  ],
  "key": ""
}